# RoboLab Frontend Production Environment Configuration
# Copy this file to .env and update with your actual values

# API Configuration
VITE_API_BASE_URL=https://your-backend-domain.com/api
VITE_WS_URL=wss://your-backend-domain.com

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_firebase_app_id
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Application Configuration
VITE_APP_NAME=RoboLabPK
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Hardware Innovation Platform
VITE_APP_URL=https://your-frontend-domain.com

# Feature Flags
VITE_FEATURE_AI_ASSISTANT=true
VITE_FEATURE_CIRCUIT_SIMULATION=true
VITE_FEATURE_REAL_TIME_COLLABORATION=true
VITE_FEATURE_PAYMENT_PROCESSING=false
VITE_FEATURE_MOBILE_APP=true
VITE_FEATURE_ANALYTICS=true
VITE_FEATURE_ERROR_TRACKING=true

# Analytics
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
VITE_MIXPANEL_TOKEN=your_mixpanel_token
VITE_HOTJAR_ID=your_hotjar_id

# Error Tracking
VITE_SENTRY_DSN=your_sentry_dsn
VITE_SENTRY_ENVIRONMENT=production
VITE_SENTRY_SAMPLE_RATE=0.1

# Performance Monitoring
VITE_PERFORMANCE_MONITORING=true
VITE_WEB_VITALS_ENABLED=true

# CDN Configuration
VITE_CDN_URL=https://your-cdn-domain.com
VITE_CDN_ENABLED=true

# Image Optimization
VITE_IMAGE_OPTIMIZATION=true
VITE_IMAGE_QUALITY=80
VITE_IMAGE_FORMAT=webp

# Caching
VITE_CACHE_ENABLED=true
VITE_CACHE_VERSION=1.0.0
VITE_CACHE_STRATEGY=stale-while-revalidate

# PWA Configuration
VITE_PWA_ENABLED=true
VITE_PWA_NAME=RoboLabPK
VITE_PWA_SHORT_NAME=RoboLab
VITE_PWA_DESCRIPTION=Hardware Innovation Platform
VITE_PWA_THEME_COLOR=#3b82f6
VITE_PWA_BACKGROUND_COLOR=#ffffff

# Service Worker
VITE_SW_ENABLED=true
VITE_SW_CACHE_NAME=robolab-cache
VITE_SW_CACHE_VERSION=1.0.0

# Offline Support
VITE_OFFLINE_ENABLED=true
VITE_OFFLINE_FALLBACK=true

# Push Notifications
VITE_PUSH_NOTIFICATIONS_ENABLED=true
VITE_VAPID_PUBLIC_KEY=your_vapid_public_key

# Social Login
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_GITHUB_CLIENT_ID=your_github_client_id
VITE_FACEBOOK_APP_ID=your_facebook_app_id

# Payment Processing
VITE_STRIPE_PUBLIC_KEY=pk_live_your_stripe_public_key
VITE_PAYPAL_CLIENT_ID=your_paypal_client_id

# Maps Integration
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_access_token

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,application/pdf,application/json
VITE_UPLOAD_CHUNK_SIZE=1048576

# Circuit Designer
VITE_CIRCUIT_MAX_COMPONENTS=1000
VITE_CIRCUIT_MAX_CONNECTIONS=2000
VITE_CIRCUIT_AUTO_SAVE_INTERVAL=30000
VITE_CIRCUIT_GRID_SIZE=20

# AI Assistant
VITE_AI_MAX_TOKENS=4000
VITE_AI_MAX_REQUESTS_PER_HOUR=100
VITE_AI_DAILY_LIMIT=50

# Real-time Features
VITE_WS_RECONNECT_ATTEMPTS=5
VITE_WS_RECONNECT_INTERVAL=5000
VITE_WS_HEARTBEAT_INTERVAL=30000

# User Interface
VITE_THEME_DEFAULT=light
VITE_THEME_OPTIONS=light,dark,system
VITE_LANGUAGE_DEFAULT=en
VITE_LANGUAGE_OPTIONS=en,ur,ar

# Performance
VITE_LAZY_LOADING=true
VITE_CODE_SPLITTING=true
VITE_PRELOADING=true
VITE_PREFETCHING=true

# Security
VITE_CSP_ENABLED=true
VITE_CSP_REPORT_URI=https://your-domain.com/csp-report
VITE_HSTS_ENABLED=true

# Development Tools (Disabled in Production)
VITE_DEVTOOLS_ENABLED=false
VITE_DEBUG_MODE=false
VITE_VERBOSE_LOGGING=false

# Build Configuration
VITE_BUILD_SOURCEMAP=false
VITE_BUILD_MINIFY=true
VITE_BUILD_COMPRESS=true

# Environment
VITE_NODE_ENV=production
VITE_DEBUG=false
VITE_VERBOSE=false

# API Configuration
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3
VITE_API_RETRY_DELAY=1000

# WebSocket Configuration
VITE_WS_TIMEOUT=30000
VITE_WS_RETRY_ATTEMPTS=5
VITE_WS_RETRY_DELAY=1000

# Storage Configuration
VITE_LOCAL_STORAGE_PREFIX=robolab_
VITE_SESSION_STORAGE_PREFIX=robolab_session_
VITE_INDEXED_DB_NAME=robolab_db
VITE_INDEXED_DB_VERSION=1

# Cache Configuration
VITE_CACHE_TTL=3600000
VITE_CACHE_MAX_SIZE=100
VITE_CACHE_CLEANUP_INTERVAL=300000

# Notification Configuration
VITE_NOTIFICATION_ENABLED=true
VITE_NOTIFICATION_DURATION=5000
VITE_NOTIFICATION_POSITION=top-right

# Loading Configuration
VITE_LOADING_DELAY=300
VITE_LOADING_TIMEOUT=30000
VITE_LOADING_RETRY_ATTEMPTS=3

# Error Configuration
VITE_ERROR_BOUNDARY_ENABLED=true
VITE_ERROR_REPORTING_ENABLED=true
VITE_ERROR_RECOVERY_ENABLED=true

# Accessibility
VITE_A11Y_ENABLED=true
VITE_A11Y_ANNOUNCEMENTS=true
VITE_A11Y_FOCUS_MANAGEMENT=true

# Internationalization
VITE_I18N_ENABLED=true
VITE_I18N_FALLBACK_LANGUAGE=en
VITE_I18N_LOADING_STRATEGY=lazy

# Testing
VITE_TESTING_ENABLED=false
VITE_TESTING_MOCK_API=false
VITE_TESTING_DEBUG=false

# Monitoring
VITE_MONITORING_ENABLED=true
VITE_MONITORING_SAMPLE_RATE=0.1
VITE_MONITORING_REPORT_INTERVAL=60000

# Logging
VITE_LOGGING_ENABLED=true
VITE_LOGGING_LEVEL=warn
VITE_LOGGING_CONSOLE=false
VITE_LOGGING_REMOTE=true

# Updates
VITE_UPDATE_CHECK_ENABLED=true
VITE_UPDATE_CHECK_INTERVAL=300000
VITE_UPDATE_AUTO_INSTALL=false

# Maintenance
VITE_MAINTENANCE_MODE=false
VITE_MAINTENANCE_MESSAGE=We are currently performing scheduled maintenance. Please check back soon.

# Feature Toggles
VITE_FEATURE_BETA_FEATURES=false
VITE_FEATURE_EXPERIMENTAL_UI=false
VITE_FEATURE_ADVANCED_ANALYTICS=false
VITE_FEATURE_ENTERPRISE_FEATURES=false

# Third-party Integrations
VITE_INTEGRATION_GOOGLE_ANALYTICS=true
VITE_INTEGRATION_GOOGLE_TAG_MANAGER=true
VITE_INTEGRATION_FACEBOOK_PIXEL=false
VITE_INTEGRATION_TWITTER_PIXEL=false
VITE_INTEGRATION_LINKEDIN_INSIGHT=false

# API Endpoints
VITE_API_AUTH_ENDPOINT=/api/auth
VITE_API_USERS_ENDPOINT=/api/users
VITE_API_PROJECTS_ENDPOINT=/api/projects
VITE_API_CIRCUITS_ENDPOINT=/api/circuits
VITE_API_COMPONENTS_ENDPOINT=/api/components
VITE_API_AI_ENDPOINT=/api/ai
VITE_API_IDEAS_ENDPOINT=/api/ideas
VITE_API_HEALTH_ENDPOINT=/api/health

# WebSocket Endpoints
VITE_WS_CIRCUIT_ENDPOINT=/socket.io/circuit
VITE_WS_CHAT_ENDPOINT=/socket.io/chat
VITE_WS_COLLABORATION_ENDPOINT=/socket.io/collaboration

# File Upload Endpoints
VITE_UPLOAD_IMAGES_ENDPOINT=/api/upload/images
VITE_UPLOAD_FILES_ENDPOINT=/api/upload/files
VITE_UPLOAD_CIRCUITS_ENDPOINT=/api/upload/circuits

# External Services
VITE_EXTERNAL_GITHUB_API=https://api.github.com
VITE_EXTERNAL_ARDUINO_API=https://api.arduino.cc
VITE_EXTERNAL_COMPONENT_API=https://api.octopart.com

# Development Server (Disabled in Production)
VITE_DEV_SERVER_PORT=5173
VITE_DEV_SERVER_HOST=localhost
VITE_DEV_SERVER_HTTPS=false
VITE_DEV_SERVER_OPEN=false

# Build Output
VITE_BUILD_OUTPUT_DIR=dist
VITE_BUILD_ASSETS_DIR=assets
VITE_BUILD_PUBLIC_DIR=public

# Source Maps
VITE_SOURCEMAP_ENABLED=false
VITE_SOURCEMAP_HIDDEN=false
VITE_SOURCEMAP_INLINE=false

# Bundle Analysis
VITE_BUNDLE_ANALYZER_ENABLED=false
VITE_BUNDLE_ANALYZER_OPEN=false
VITE_BUNDLE_ANALYZER_PORT=8888

# Type Checking
VITE_TYPE_CHECK_ENABLED=false
VITE_TYPE_CHECK_STRICT=false
VITE_TYPE_CHECK_NO_EMIT=false

# Linting
VITE_ESLINT_ENABLED=false
VITE_ESLINT_FIX=false
VITE_ESLINT_QUIET=false

# Formatting
VITE_PRETTIER_ENABLED=false
VITE_PRETTIER_WRITE=false
VITE_PRETTIER_CHECK=false
